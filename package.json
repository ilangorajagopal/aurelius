    "generate:css": "tailwindcss -i ./styles/tailwind.css -o ./app/tailwind.css",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "prepare": "husky install",
    "postinstall": "remix setup node",
    "typecheck": "tsc -b"
  },
